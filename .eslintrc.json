{
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true
	},
	"extends": ["airbnb-base", "prettier"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"prefer-const": [
			"warn",
			{
				"destructuring": "any",
				"ignoreReadBeforeAssign": false
			}
		]
	}
}
